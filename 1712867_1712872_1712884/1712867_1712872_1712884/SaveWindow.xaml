<Window x:Class="_1712867_1712872_1712884.SaveWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_1712867_1712872_1712884"
        mc:Ignorable="d"
        Title="SaveWindow" ResizeMode="NoResize" Height="200" Width="320"
        Icon="/Images/save.png" FontFamily="Segoe UI Emoji"
        BorderThickness="2" WindowStartupLocation="CenterScreen"
        BorderBrush="Purple" Background="LightBlue">
    <Canvas>
        <TextBlock Text="Enter playlist's name:" VerticalAlignment="Top" Margin="40,70,0,0"/>
        <TextBox Name="PlaylistNameTextBox" Width="100" VerticalAlignment="Top" Margin="160,70,0,0"/>
        <Button Content="Save" Width="80" Height="25" VerticalAlignment="Top" Margin="50,110,0,0" IsDefault="True" Name="OKButton" Click="OKButton_Click"></Button>
        <Button Content="Cancel" Width="80" Height="25" VerticalAlignment="Top" Margin="180,110,0,0" IsCancel="True" Name="CancelButton" Click="CancelButton_Click"></Button>
    </Canvas>
</Window>
